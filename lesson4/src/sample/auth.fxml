<?xml version="1.0" encoding="UTF-8"?>

<?scenebuilder-stylesheet /css/Style.css ?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="center" prefHeight="140.0" prefWidth="359.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ControllerAuth">
    <stylesheets>
        <URL value="@/css/Style.css" />
    </stylesheets>
     <Label prefHeight="28.0" prefWidth="700.0" text="Введите ник: ">
    <VBox.margin>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </VBox.margin></Label>
    <HBox>
        <TextField fx:id="textAuth" onAction="#sendNick" promptText="Введите ник..." HBox.hgrow="ALWAYS">
        <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </HBox.margin>
        </TextField>
        <Button fx:id="sendButton" onAction="#sendNick" text="Продолжить">
        <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </HBox.margin>
        </Button>
    </HBox>
</VBox>
